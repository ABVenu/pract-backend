{
  "family": "prca-demo-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::804878519643:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "prac-backend",
      "image": "804878519643.dkr.ecr.eu-north-1.amazonaws.com/prac-backend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "S3_BUCKET", "value": "pract-demo-profilepic" },
        { "name": "AWS_REGION", "value": "eu-north-1" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/prca-demo-backend",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}